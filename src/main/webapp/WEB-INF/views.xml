<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="errro403" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/common/403.jsp"/>
    </bean>
    <bean id="errro404" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/common/404.jsp"/>
    </bean>
    <bean id="errro500" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/common/500.jsp"/>
    </bean>

    <bean id="wapindex" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/pages/wap/views/index.jsp"/>
    </bean>

    <bean id="login" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/pages/wap/views/login.jsp"/>
    </bean>

    <bean id="register" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/pages/wap/views/register.jsp"/>
    </bean>

    <bean id="customer" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/pages/wap/views/customer.jsp"/>
    </bean>

    <bean id="address" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/pages/wap/views/address.jsp"/>
    </bean>

    <bean id="successRedirect" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/pages/wap/views/successRedirect.jsp"/>
    </bean>

    <bean id="editAddress" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/pages/wap/views/editAddress.jsp"/>
    </bean>

    <bean id="mainFrame" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/pages/wap/mainFrame.jsp"/>
    </bean>
</beans>
